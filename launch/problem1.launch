<launch>
    <arg name="model" default="burger"/>
    <env name="TURTLEBOT3_MODEL" value="$(arg model)"/>

    <node pkg="turtlebot3_gazebo" type="turtlebot3_house.launch" name="turtlebot3_house" launch-prefix="roslaunch" />
    <node pkg="hw_pkg" type="move_turtlebot.py" name="move_turtlebot" output="screen" />
</launch>